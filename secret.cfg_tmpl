[account]
user = # Your trac username
pass = # Your trac password
loginurl = # The url to your trac
reporturl = # The report to use for open tickets

[mail]
mailhost = # The mailhost, assumes it uses starttls
port = # The port to connect to
user = # The username for smtp login
pass = # The password for smtp login
from = # The from adress
to = # The to adress

[config]
urgent_after_n_days = # How old must a ticket be to consider urgent
users_to_ignore = # semicolon separated list of user names
